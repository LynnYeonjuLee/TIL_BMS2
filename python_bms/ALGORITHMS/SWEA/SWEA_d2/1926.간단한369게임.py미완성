N = int(input())
num_list = []
for i in range(1, N+1):
    
    if (i % 10) == 3 or (i % 10) == 6 or (i % 10) == 9:
        num_list.append('-')
    else: 
        num_list.append(i)
    if (i // 10) == 3 or (i // 10) == 6 or (i // 10) == 9:
        num_list.append('-') 
        
    
print(num_list)
        