T = int(input())
for test_case in range(1, T+1):
    A, B = list(map(int,input().split()))
    N = len(A)
    M = len(B)
    move = N - M 
    sum = 0
    if move > 0:
        for i in range(N-M+1):
            for j in range(M):
                sum += A[i] * B[j]
    elif move == 0:
        for i in range(N):
            sum += A[i] * B[i]



